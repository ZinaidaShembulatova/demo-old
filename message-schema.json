{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"status": {
			"type": "string"
		},
		"data": {
			"type": "array",
			"items": [
				{
					"type": "object",
					"properties": {
						"name": {
							"type": "string",
							"maxLength": 50
						},
						"partitions": {
							"type": "integer"
						}
					},
					"required": [
						"name",
						"partitions"
					]
				}
			]
		}
	},
	"required": [
		"status",
		"data"
	]
}